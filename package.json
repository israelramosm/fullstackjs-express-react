{
  "name": "fullstackjs",
  "version": "1.0.0",
  "description": "A express template to learn more about Javascript",
  "main": "index.js",
  "scripts": {
    "jest:client": "jest --config ./__tests__/client.config.js --passWithNoTests",
    "jest:node": "jest --config ./__tests__/node.config.js --passWithNoTests",
    "clean": "rimraf dist",
    "db": "mongod --auth --dbpath \"/home/israel/Documents/mongodb/databases\"",
    "coverage": "jest --coverage --testRegex __tests__/*.tests/*.test.js",
    "build:dev": "npm run clean && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "build:prod": "npm run clean && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "start": "node ./dist/server.js",
    "standardx": "standardx",
    "standardx:fix": "standardx --fix",
    "nodemon": "nodemon --watch dist/server.js --delay 2500ms ./dist/server.js",
    "watch:backEnd": "webpack --watch --mode development --config webpack.server.config.js",
    "watch:frontEnd": "webpack --watch --mode development --config webpack.dev.config.js",
    "start:backEnd": "npm-run-all -p db nodemon watch:backEnd",
    "startAll": "npm-run-all -p db nodemon watch:backEnd watch:frontEnd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Israel-94/express-template.git"
  },
  "author": "Israel Ramos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Israel-94/express-template/issues"
  },
  "homepage": "https://github.com/Israel-94/express-template#readme",
  "dependencies": {
    "carbon-components": "^10.28.0",
    "carbon-components-react": "^7.27.0",
    "carbon-icons": "^7.0.7",
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@hapi/joi": "^17.1.1",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "mini-css-extract-plugin": "^1.3.3",
    "mongodb-memory-server": "^6.9.2",
    "mongoose": "^5.11.8",
    "moxios": "^0.4.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "react-router-dom": "^5.2.0",
    "redux-devtools": "^3.7.0",
    "sass-loader": "^10.1.0",
    "standardx": "^7.0.0",
    "style-loader": "^2.0.0",
    "supertest": "^6.0.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^2.5.2"
  }
}
